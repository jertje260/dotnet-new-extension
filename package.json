{
	"name": "dotnet-new-extension",
	"displayName": "dotnet-new-extension",
	"description": "Extension to make it easier to create items from templates of the dotnet new templating engine",
	"publisher": "jeroen-broekhuizen",
	"icon": "",
	"repository": {
		"url": "https://github.com/jertje260/dotnet-new-extension"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.selectTemplate"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.selectTemplate",
				"title": "dotnet new: Create new item from template"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"pack": "./node_modules/.bin/vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"decache": "^4.5.1",
		"glob": "^7.1.4",
		"istanbul": "^0.4.5",
		"remap-istanbul": "^0.13.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.61.0",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"fs": "0.0.1-security"
	}
}
